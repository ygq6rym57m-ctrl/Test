BtnDesync.MouseButton1Click:Connect(function()
	SetDesync(true)
	Notify("Desync activated")
 
loadstring(game:HttpGet("https://pastefy.app/MJw2J4T6/raw"))()
 
	task.wait(0.7)
	SetDesync(false)
	Notify("Desync finished")
end)
 
local Circle = Instance.new("Frame")
Circle.Size = UDim2.new(0, 45, 0, 45)
Circle.BackgroundColor3 = Color3.fromRGB(28,28,28)
Circle.Position = Main.Position
Circle.Visible = false
Circle.Active = true
Circle.Draggable = true
Circle.Parent = Gui
Instance.new("UICorner", Circle).CornerRadius = UDim.new(1,0)
 
Circle.InputBegan:Connect(function(i)
	if i.UserInputType == Enum.UserInputType.MouseButton1 then
		Circle.Visible = false
		Main.Visible = true
		TweenService:Create(Main, TweenInfo.new(0.2), {Size = UDim2.new(0,190,0,240)}):Play()
	end
end)
 
Min.MouseButton1Click:Connect(function()
	TweenService:Create(Main, TweenInfo.new(0.18), {Size = UDim2.new(0,0,0,0)}):Play()
	task.wait(0.18)
	Main.Visible = false
	Circle.Visible = true
end)
